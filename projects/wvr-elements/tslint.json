{
  "extends": "../../tslint.json",
  "rules": {
    "directive-selector": [
      true,
      "attribute",
      "wvr",
      "camelCase"
    ],
    "component-selector": [
      true,
      "element",
      "wvr",
      "kebab-case"
    ],
    "no-floating-promises": false,
    "no-implicit-dependencies": false,
    "no-inputs-metadata-property": false,
    "no-host-metadata-property": false
  }
}