{
  "name": "weaver-components",
  "version": "0.0.1",
  "scripts": {
    "build": "ng build --prod --output-hashing none",
    "build:clean": "npm run clean && npm run build",
    "build:wvr-elements": "ng build wvr-elements",
    "build:wvr-components": "node bld/build-wvr-components.js",
    "clean": "rm -rf dist",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild": "npm run build:wvr-elements",
    "postbuild": "npm run build:wvr-components",
    "start": "ng serve",
    "start:docker": "docker run  -p 80:80 --name weaver-components-1  weaver-components",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "ng e2e",
    "test:unit": "npm run test:watch -- --watch=false",
    "test:watch": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/elements": "^8.2.14",
    "@angular/forms": "~8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@ng-bootstrap/ng-bootstrap": "^5.1.5",
    "@webcomponents/shadydom": "^1.7.1",
    "@webcomponents/webcomponentsjs": "^2.4.1",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.7.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.21",
    "@angular-devkit/build-ng-packagr": "~0.803.22",
    "@angular/cli": "~8.3.21",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "concat": "^1.0.3",
    "fs-extra": "^8.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "^0.37.0",
    "tslint": "~5.15.0",
    "typescript": "~3.5.3"
  }
}
