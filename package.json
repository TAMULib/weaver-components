{
  "name": "weaver-components",
  "version": "0.0.1",
  "private": false,
  "license": "MIT",
  "scripts": {
    "build": "ng build --prod --output-hashing none",
    "build:clean": "npm run clean && npm run build",
    "build:wvr-elements": "ng build wvr-elements",
    "build:wvr-components": "node scripts/build-wvr-components.js",
    "clean": "rm -rf dist",
    "lint": "ng lint",
    "ng": "ng",
    "prebuild": "npm run build:wvr-elements",
    "postbuild": "npm run build:wvr-components",
    "start": "ng serve --port 4200",
    "start:static": "static-server -p 9080",
    "start:docker": "docker run  -p 80:80 --name weaver-components-1  weaver-components",
    "test": "npm run test:unit && npm run test:e2e",
    "test:e2e": "ng e2e",
    "test:unit": "ng test --no-watch",
    "test:watch": "ng test",
    "test:coverage": "ng test --no-watch --code-coverage",
    "test:report": "cat ./coverage/wvr-elements/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:ci": "npm run build && npm run test:coverage",
    "storybook": "start-storybook -p 6006 -s ./dist",
    "build-storybook": "build-storybook"
  },
  "dependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/common": "^9.0.0",
    "@angular/compiler": "^9.0.0",
    "@angular/core": "^9.0.0",
    "@angular/elements": "^9.0.0",
    "@angular/forms": "^9.0.0",
    "@angular/platform-browser": "^9.0.0",
    "@angular/platform-browser-dynamic": "^9.0.0",
    "@angular/router": "^9.0.0",
    "@ng-bootstrap/ng-bootstrap": "^5.2.2",
    "@wvr/elements": "0.0.1",
    "bootstrap": "^4.4.1",
    "classlist.js": "^1.1.20150312",
    "document-register-element": "^1.14.3",
    "ie11-custom-properties": "^3.0.6",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "^9.0.1",
    "@angular/compiler-cli": "^9.0.0",
    "@angular/language-service": "^9.0.0",
    "@babel/core": "^7.8.4",
    "@pickra/copy-code-block": "^1.2.0",
    "@storybook/addon-storysource": "next",
    "@storybook/html": "next",
    "@types/jasmine": "~3.5.3",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.7.0",
    "angular-tslint-rules": "^1.20.3",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.2.1",
    "concat": "^1.0.3",
    "coveralls": "^3.0.9",
    "highlight.js": "^9.18.1",
    "hljs": "^6.2.3",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.0",
    "karma-jasmine-html-reporter": "^1.5.2",
    "ng-packagr": "^9.0.0",
    "protractor": "^5.4.3",
    "ts-node": "~8.6.2",
    "tsickle": "^0.38.0",
    "tslint": "~6.0.0",
    "tslint-angular": "^3.0.2",
    "typescript": "^3.7.5"
  }
}
